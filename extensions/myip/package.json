{
  "$schema": "https://jarvis-extensions.huakun.tech/jarvis-ext-package-json-schema.json",
  "name": "@huakunshen/jarvis-ext-myip",
  "version": "0.0.1",
  "description": "Jarvis IP Extension",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@vueuse/core": "^10.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jarvis-api": "0.0.2-alpha.1",
    "radix-vue": "^1.8.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.21",
    "zod": "^3.23.8"
  },
  "jarvis": {
    "identifier": "tech.huakun.jarvis-qrcode-ext",
    "demoImages": [],
    "icon": {
      "type": "iconify",
      "icon": "mdi:ip-network"
    },
    "uiCmds": [
      {
        "main": "dist",
        "name": "My IP",
        "devMain": "http://localhost:5173",
        "window": {
          "titleBarStyle": "overlay"
        },
        "cmds": [
          {
            "type": "text",
            "value": "ip"
          }
        ]
      }
    ]
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "^1.1.14",
    "@iconify/vue": "^4.1.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.11",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "run-p": "^0.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.11"
  },
  "files": [
    "dist"
  ]
}