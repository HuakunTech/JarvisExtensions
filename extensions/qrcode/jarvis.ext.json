        {
            "main": "dist/detect-qrcode.html",
            "name": "Detect QRCode",
            "devMain": "http://localhost:5173/detect-qrcode",
            "window": {
                "titleBarStyle": "overlay"
            },
            "cmds": [
                {
                    "type": "text",
                    "value": "read detect qrcode"
                }
            ]
        }
    ],
    "inlineCmds": []
